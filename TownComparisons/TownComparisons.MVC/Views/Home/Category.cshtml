@using TownComparisons.Domain.Entities;
@using TownComparisons.Domain.WebServices.Models
@{
    ViewBag.Title = "Kategori";
}

<h3>Kategori</h3>
<div id="Categories">
    @{
        foreach (OrganisationalUnit ou in ViewBag.OUnits)
        {
            <a href="~/Home/AddOrganisationalUnit/@ou.Id">@ou.Title</a><br />
        }
    }
</div>